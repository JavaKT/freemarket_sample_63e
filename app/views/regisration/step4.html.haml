= form_for @address, url: step5_regisration_index_path, method: :post, html: {class: 'second-wrapper__box'} do |f|
  = current_user.nickname
  都道府県
  = f.collection_select :prefecture_id, Prefecture.all, :id, :name
  郵便番号
  = f.text_field :post_no
  市町村区
  = f.text_field :city
  番地以下
  = f.text_field :town
  建物名など
  = f.text_field :building
  電話番号
  = f.text_field :tell
  = f.submit "次へ"